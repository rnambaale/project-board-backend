{
  "name": "project-board-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d lib",
    "pretest": "NODE_ENV=test npm run undo:migrate && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed",
    "test": "NODE_ENV=test nyc mocha --timeout 50000 --require @babel/register --exit",
    "start": "npm run build && node lib/server nodemon --exec",
    "start:dev": "nodemon --exec babel-node src/server",
    "migrate": "npx sequelize db:migrate",
    "undo:migrate": "npx sequelize db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate:fresh": "npm run undo:migrate && npm run migrate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "nodemailer": "^6.3.0",
    "pg": "^7.11.0",
    "pg-connection-string": "^2.1.0",
    "pg-hstore": "^2.3.3",
    "pusher": "^2.2.1",
    "request": "^2.87.0",
    "sendmail": "^1.6.1",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
